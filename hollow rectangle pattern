public class patterns2 {
    public static void main(String[] args) {
        int totalrow = 4;
        int totalcol = 5;

        for (int i = 1; i <= totalrow; i++) {
            for (int j = 1; j <= totalcol; j++) {
                if (i == 1 || i == totalrow || j == 1 || j == totalcol) {
                    System.out.print("* ");
                } else {
                    System.out.print("  ");
                }
            }
            System.out.println(); 
        }
    }
}
